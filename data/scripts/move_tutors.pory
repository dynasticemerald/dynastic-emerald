script BugMoves_Tutor{
	lock
	faceplayer
	msgbox(format("Hello! My Bug type Pokémon LOVE Oran Berries! If you give me one, I can teach some Bug-Type moves to one of your Pokémon."), MSGBOX_DEFAULT)
	checkitem(ITEM_ORAN_BERRY, 1)
	if(var(VAR_RESULT) == 0){
		msgbox(format("You don't carry oran berries?\nAnd you call Yourself a Pokémon Trainer. mphm."), MSGBOX_DEFAULT)
		release
		end
	}
	else{
		msgbox(format("Oh! An Oran Berry! I can teach a good Number of Bug-Type moves in exchange for one of those!"), MSGBOX_YESNO)
		if(var(VAR_RESULT) == NO){
			msgbox(format("Aw, shooks my weedle was so hungry, you monster >:("), MSGBOX_DEFAULT)
			release
			end
		}
		else{
		msgbox(format("Great! Wich move should i teach to one of Your Pokémon?"), MSGBOX_DEFAULT)
		dynmultichoice(0,0,FALSE,4,0,DYN_MULTICHOICE_CB_NONE, "Bug Bite", "Silver Wind", "Pounce", "Struggle Bug")
			switch (var(VAR_RESULT)){
				case 0: setvar (VAR_0x8005, MOVE_BUG_BITE) special (ChooseMonForMoveTutor) waitstate if(var(VAR_RESULT) == MULTI_B_PRESSED){ msgbox(format("Aw, shooks my Weedle is at 1 hp, you monster >:("), MSGBOX_DEFAULT) release end } else { removeitem(ITEM_ORAN_BERRY, 1) msgbox(format("Thank you! now my weedle can be Healed Up!"), MSGBOX_DEFAULT) release end}
				case 1: setvar (VAR_0x8005, MOVE_SILVER_WIND) special (ChooseMonForMoveTutor) waitstate if(var(VAR_RESULT) == MULTI_B_PRESSED){ msgbox(format("Aw, shooks my Weedle is at 1 hp, you monster >:("), MSGBOX_DEFAULT) release end } else { removeitem(ITEM_ORAN_BERRY, 1) msgbox(format("Thank you! now my weedle can be Healed Up!"), MSGBOX_DEFAULT) release end}
				case 2: setvar (VAR_0x8005, MOVE_POUNCE) special (ChooseMonForMoveTutor) waitstate if(var(VAR_RESULT) == MULTI_B_PRESSED){ msgbox(format("Aw, shooks my Weedle is at 1 hp, you monster >:("), MSGBOX_DEFAULT) release end } else { removeitem(ITEM_ORAN_BERRY, 1) msgbox(format("Thank you! now my weedle can be Healed Up!"), MSGBOX_DEFAULT) release end}
				case 3: setvar (VAR_0x8005, MOVE_STRUGGLE_BUG) special (ChooseMonForMoveTutor) waitstate if(var(VAR_RESULT) == MULTI_B_PRESSED){ msgbox(format("Aw, shooks my Weedle is at 1 hp, you monster >:("), MSGBOX_DEFAULT) release end } else { removeitem(ITEM_ORAN_BERRY, 1) msgbox(format("Thank you! now my weedle can be Healed Up!"), MSGBOX_DEFAULT) release end}
			}
		}
	}
}

